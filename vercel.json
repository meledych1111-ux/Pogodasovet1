{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/save-score.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/save-progress.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/get-progress.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/user-stats.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/top-players.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/check-db.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/game",
      "dest": "/public/index.html"
    },
    {
      "src": "/tetris",
      "dest": "/public/index.html"
    },
    {
      "src": "/bot",
      "dest": "/api/bot.js"
    },
    {
      "src": "/api/bot",
      "dest": "/api/bot.js"
    },
    {
      "src": "/api/save-score",
      "dest": "/api/save-score.js"
    },
    {
      "src": "/api/save-progress",
      "dest": "/api/save-progress.js"
    },
    {
      "src": "/api/get-progress",
      "dest": "/api/get-progress.js"
    },
    {
      "src": "/api/user-stats",
      "dest": "/api/user-stats.js"
    },
    {
      "src": "/api/top-players",
      "dest": "/api/top-players.js"
    },
    {
      "src": "/api/check-db",
      "dest": "/api/check-db.js"
    },
    {
      "src": "/api/(.*)",
      "status": 404
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-Requested-With, Content-Type, Accept, Authorization"
        }
      ]
    }
  ]
}
